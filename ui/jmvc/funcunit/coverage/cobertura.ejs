<?xml version="1.0"?>
<!--DOCTYPE coverage SYSTEM "http://cobertura.sourceforge.net/xml/coverage-03.dtd"-->

<coverage line-rate="0.9" branch-rate="0.0">
	<sources>
		<source><%= steal.File.cwd() %></source>
		<source>--source</source>
	</sources>
	<packages>
		<package name="" line-rate="<%= parseInt(total.pct, 10)/100 %>" branch-rate="0.0" complexity="0.0">
			<classes>
			<% for(var file in files){ %>
				<class name="" filename="<%= file %>" line-rate="<%= parseInt(files[file].stats.pct, 10)/100 %>" branch-rate="0.0" complexity="0.0">
					<lines>
						<% for(var i=0; i < files[file].lines.length; i++) { %>
						<% if(files[file].lines[i] !== null){ %>
						<line number="<%= i %>" hits="<%= files[file].lines[i] %>" branch="false"/>
						<% } %>
						<% } %>
					</lines>
				</class>
			<% } %>
			</classes>
		</package>
	</packages>
</coverage>